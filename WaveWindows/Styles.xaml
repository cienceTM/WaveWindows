<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:controls="clr-namespace:WaveWindows.Controls"
                    xmlns:card="clr-namespace:WaveWindows.Controls.Card"
                    xmlns:editor="clr-namespace:WaveWindows.Controls.Editor"
                    xmlns:settings="clr-namespace:WaveWindows.Controls.Settings"
                    xmlns:toast="clr-namespace:WaveWindows.Controls.Toast"
                    xmlns:converters="clr-namespace:WaveWindows.Converters"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <converters:BrushToColorConverter
        x:Key="BrushToColorConverter" />
    <converters:NotEqualToConverter
        x:Key="NotEqualToConverter" />
    <converters:ThicknessToCornerRadius
        x:Key="ThicknessTocornerRadius" />
    <Style
        TargetType="{x:Type controls:Button}"
        x:Key="{x:Type controls:Button}">
        <Setter
            Property="FrameworkElement.FocusVisualStyle"
            Value="{x:Null}" />
        <Setter
            Property="HorizontalContentAlignment"
            Value="Center" />
        <Setter
            Property="VerticalContentAlignment"
            Value="Center" />
        <Setter
            Property="Padding"
            Value="1" />
        <Setter
            Property="Template">
            <Setter.Value>
                <ControlTemplate
                    TargetType="{x:Type controls:Button}">
                    <Border
                        Background="{TemplateBinding Background}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="{TemplateBinding BorderThickness}"
                        SnapsToDevicePixels="True"
                        CornerRadius="{Binding CornerRadius, RelativeSource={RelativeSource TemplatedParent}}">
                        <ContentPresenter
                            Focusable="False"
                            SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                            HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                            Margin="{TemplateBinding Padding}" />
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style
        TargetType="{x:Type controls:Button}"
        x:Key="IconButton">
        <Setter
            Property="FrameworkElement.FocusVisualStyle"
            Value="{x:Null}" />
        <Setter
            Property="UIElement.SnapsToDevicePixels"
            Value="True" />
        <Setter
            Property="FrameworkElement.OverridesDefaultStyle"
            Value="True" />
        <Setter
            Property="Background"
            Value="#00FFFFFF" />
        <Setter
            Property="Foreground"
            Value="#B2B2B2" />
        <Setter
            Property="Template">
            <Setter.Value>
                <ControlTemplate
                    TargetType="{x:Type controls:Button}">
                    <Border
                        Background="{TemplateBinding Background}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="{TemplateBinding BorderThickness}"
                        SnapsToDevicePixels="True"
                        CornerRadius="{Binding CornerRadius, RelativeSource={RelativeSource TemplatedParent}}">
                        <Path
                            Fill="{TemplateBinding Foreground}"
                            Stretch="Uniform"
                            Margin="{TemplateBinding Padding}"
                            Data="{Binding Content, RelativeSource={RelativeSource Mode=TemplatedParent}}" />
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger
                            Property="UIElement.IsMouseOver"
                            Value="True">
                            <TriggerBase.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ColorAnimation
                                            To="#FFFFFF"
                                            Duration="0:0:0.2"
                                            Storyboard.TargetProperty="(Control.Foreground).(SolidColorBrush.Color)" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </TriggerBase.EnterActions>
                            <TriggerBase.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ColorAnimation
                                            To="#B2B2B2"
                                            Duration="0:0:0.2"
                                            Storyboard.TargetProperty="(Control.Foreground).(SolidColorBrush.Color)" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </TriggerBase.ExitActions>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style
        TargetType="{x:Type ToggleButton}"
        x:Key="IconToggleButton">
        <Setter
            Property="FrameworkElement.FocusVisualStyle"
            Value="{x:Null}" />
        <Setter
            Property="UIElement.SnapsToDevicePixels"
            Value="True" />
        <Setter
            Property="FrameworkElement.OverridesDefaultStyle"
            Value="True" />
        <Setter
            Property="Background"
            Value="#00FFFFFF" />
        <Setter
            Property="Foreground"
            Value="#B2B2B2" />
        <Setter
            Property="Template">
            <Setter.Value>
                <ControlTemplate
                    TargetType="{x:Type ToggleButton}">
                    <Border
                        Background="{TemplateBinding Background}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="0"
                        SnapsToDevicePixels="True"
                        CornerRadius="{Binding BorderThickness, RelativeSource={RelativeSource Mode=TemplatedParent}}">
                        <Path
                            Fill="{TemplateBinding Foreground}"
                            Stretch="Uniform"
                            Margin="{TemplateBinding Padding}"
                            Data="{Binding Content, RelativeSource={RelativeSource Mode=TemplatedParent}}" />
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger
                            Property="ToggleButton.IsChecked"
                            Value="True">
                            <TriggerBase.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ColorAnimation
                                            To="#FFFFFF"
                                            Duration="0:0:0.2"
                                            Storyboard.TargetProperty="(Control.Foreground).(SolidColorBrush.Color)" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </TriggerBase.EnterActions>
                            <TriggerBase.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ColorAnimation
                                            To="#B2B2B2"
                                            Duration="0:0:0.2"
                                            Storyboard.TargetProperty="(Control.Foreground).(SolidColorBrush.Color)" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </TriggerBase.ExitActions>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style
        TargetType="{x:Type controls:InputBox}"
        x:Key="{x:Type controls:InputBox}">
        <Setter
            Property="FrameworkElement.FocusVisualStyle"
            Value="{x:Null}" />
        <Setter
            Property="Background"
            Value="#EAF0F7" />
        <Setter
            Property="BorderBrush"
            Value="#00FFFFFF" />
        <Setter
            Property="BorderThickness"
            Value="0" />
        <Setter
            Property="UIElement.AllowDrop"
            Value="True" />
        <Setter
            Property="Padding"
            Value="10,5,35,0" />
        <Setter
            Property="HorizontalContentAlignment"
            Value="Left" />
        <Setter
            Property="VerticalContentAlignment"
            Value="Center" />
        <Setter
            Property="Template">
            <Setter.Value>
                <ControlTemplate
                    TargetType="{x:Type controls:InputBox}">
                    <Border
                        Name="Background"
                        Background="{TemplateBinding Background}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="{TemplateBinding BorderThickness}"
                        CornerRadius="{Binding CornerRadius, RelativeSource={RelativeSource TemplatedParent}}">
                        <Grid>
                            <ScrollViewer
                                Name="PART_ContentHost"
                                Focusable="False"
                                HorizontalScrollBarVisibility="Hidden"
                                VerticalScrollBarVisibility="Hidden"
                                HorizontalContentAlignment="Left"
                                VerticalContentAlignment="Center"
                                Margin="{TemplateBinding Padding}" />
                            <TextBlock
                                Name="Placeholder"
                                FontFamily="{TemplateBinding FontFamily}"
                                FontSize="{TemplateBinding FontSize}"
                                FontWeight="{TemplateBinding FontWeight}"
                                Visibility="Collapsed"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Margin="12,0,0,0"
                                Text="{Binding Placeholder, RelativeSource={RelativeSource TemplatedParent}}"
                                Foreground="{Binding PlaceholderForeground, RelativeSource={RelativeSource TemplatedParent}}"
                                FontStyle="{Binding PlaceholderStyle, RelativeSource={RelativeSource TemplatedParent}}" />
                            <Border
                                Name="ClearButton"
                                Height="15"
                                Width="15"
                                Background="#00FFFFFF"
                                BorderBrush="#667085"
                                BorderThickness="1"
                                CornerRadius="17.5"
                                Opacity="1"
                                HorizontalAlignment="Right"
                                VerticalAlignment="Center"
                                Margin="0,0,10,0"
                                Visibility="{Binding ClearButtonVisibility, RelativeSource={RelativeSource TemplatedParent}}">
                                <Grid>
                                    <Path
                                        Stroke="#667085"
                                        StrokeStartLineCap="Round"
                                        StrokeEndLineCap="Round"
                                        StrokeLineJoin="Round"
                                        Margin="-2.75,-2.75,0,0"
                                        Data="M11.7944,7.00214 L7.00244,11.7941" />
                                    <Path
                                        Stroke="#667085"
                                        StrokeStartLineCap="Round"
                                        StrokeEndLineCap="Round"
                                        StrokeLineJoin="Round"
                                        Margin="-2.75,-2.75,0,0"
                                        Data="M11.796,11.797 L7,7" />
                                </Grid>
                            </Border>
                        </Grid>
                    </Border>
                    <ControlTemplate.Triggers>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition
                                    Property="UIElement.IsKeyboardFocused"
                                    Value="False" />
                                <Condition
                                    Value="{x:Static system:String.Empty}"
                                    Property="Text" />
                            </MultiTrigger.Conditions>
                            <Setter
                                TargetName="Placeholder"
                                Property="Visibility"
                                Value="Visible" />
                        </MultiTrigger>
                        <Trigger
                            Value="{x:Static system:String.Empty}"
                            Property="Text">
                            <TriggerBase.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation
                                            To="0"
                                            Duration="0:0:0.25"
                                            Storyboard.TargetName="ClearButton"
                                            Storyboard.TargetProperty="Opacity" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </TriggerBase.EnterActions>
                            <TriggerBase.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation
                                            To="1"
                                            Duration="0:0:0.25"
                                            Storyboard.TargetName="ClearButton"
                                            Storyboard.TargetProperty="Opacity" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </TriggerBase.ExitActions>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style
        TargetType="{x:Type controls:Message}"
        x:Key="{x:Type controls:Message}">
        <Setter
            Property="FrameworkElement.FocusVisualStyle"
            Value="{x:Null}" />
        <Setter
            Property="UIElement.SnapsToDevicePixels"
            Value="True" />
        <Setter
            Property="Width"
            Value="200" />
        <Setter
            Property="HorizontalAlignment"
            Value="Left" />
        <Setter
            Property="VerticalAlignment"
            Value="Top" />
        <Setter
            Property="Background"
            Value="#00FFFFFF" />
        <Setter
            Property="BorderBrush"
            Value="#00FFFFFF" />
        <Setter
            Property="BorderThickness"
            Value="0" />
        <Setter
            Property="Padding"
            Value="4,1" />
        <Setter
            Property="Margin"
            Value="0,0,0,10" />
        <Setter
            Property="Template">
            <Setter.Value>
                <ControlTemplate
                    TargetType="{x:Type controls:Message}">
                    <Border
                        Name="Background"
                        Background="{TemplateBinding Background}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="{TemplateBinding BorderThickness}"
                        Padding="{TemplateBinding Padding}"
                        SnapsToDevicePixels="True">
                        <Grid>
                            <Border
                                Name="Indicator"
                                Width="2.5"
                                Background="#267DE5"
                                CornerRadius="1.5"
                                HorizontalAlignment="Left" />
                            <TextBlock
                                Name="Message"
                                TextWrapping="Wrap"
                                Foreground="#FFFFFF"
                                FontFamily="Izmir"
                                FontSize="14"
                                Margin="10,0,10,0"
                                Text="{Binding Text, RelativeSource={RelativeSource TemplatedParent}}" />
                        </Grid>
                    </Border>
                    <ControlTemplate.Triggers>
                        <DataTrigger
                            Value="True"
                            Binding="{Binding Reverse, RelativeSource={RelativeSource Self}}">
                            <DataTrigger.Setters>
                                <Setter
                                    TargetName="Indicator"
                                    Property="HorizontalAlignment"
                                    Value="Right" />
                                <Setter
                                    TargetName="Indicator"
                                    Property="Width"
                                    Value="3.5" />
                                <Setter
                                    TargetName="Message"
                                    Property="Width"
                                    Value="175" />
                                <Setter
                                    TargetName="Message"
                                    Property="TextBlock.TextAlignment"
                                    Value="Right" />
                                <Setter
                                    TargetName="Message"
                                    Property="Margin"
                                    Value="0,0,0,0" />
                            </DataTrigger.Setters>
                        </DataTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
